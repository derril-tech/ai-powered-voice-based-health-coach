# Frontend Environment Variables for AI Voice Assistant

# Application Settings
NEXT_PUBLIC_APP_NAME=AI Voice Assistant
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_ENVIRONMENT=development

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_API_VERSION=v1
NEXT_PUBLIC_WS_URL=ws://localhost:8000/ws
NEXT_PUBLIC_SOCKET_URL=http://localhost:8000

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-key-here
NEXTAUTH_JWT_SECRET=your-jwt-secret-key-here

# OAuth Providers
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

MICROSOFT_CLIENT_ID=your-microsoft-client-id
MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret

GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# AI Services
NEXT_PUBLIC_OPENAI_API_KEY=your-openai-api-key
NEXT_PUBLIC_ANTHROPIC_API_KEY=your-anthropic-api-key

# Voice Services
NEXT_PUBLIC_AZURE_SPEECH_KEY=your-azure-speech-key
NEXT_PUBLIC_AZURE_SPEECH_REGION=your-azure-speech-region

NEXT_PUBLIC_GOOGLE_SPEECH_API_KEY=your-google-speech-api-key

# Calendar Integration
NEXT_PUBLIC_GOOGLE_CALENDAR_CLIENT_ID=your-google-calendar-client-id
NEXT_PUBLIC_GOOGLE_CALENDAR_API_KEY=your-google-calendar-api-key

NEXT_PUBLIC_MICROSOFT_GRAPH_CLIENT_ID=your-microsoft-graph-client-id
NEXT_PUBLIC_MICROSOFT_GRAPH_TENANT_ID=your-microsoft-graph-tenant-id

# File Storage
NEXT_PUBLIC_AWS_S3_BUCKET=your-aws-s3-bucket
NEXT_PUBLIC_AWS_S3_REGION=your-aws-s3-region
NEXT_PUBLIC_AWS_CLOUDFRONT_URL=your-aws-cloudfront-url

NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET=your-cloudinary-upload-preset

# Email Services
NEXT_PUBLIC_SENDGRID_API_KEY=your-sendgrid-api-key
NEXT_PUBLIC_SENDGRID_FROM_EMAIL=noreply@voiceassistant.com

NEXT_PUBLIC_AWS_SES_REGION=your-aws-ses-region
NEXT_PUBLIC_AWS_SES_FROM_EMAIL=noreply@voiceassistant.com

# Analytics & Monitoring
NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn
NEXT_PUBLIC_POSTHOG_KEY=your-posthog-key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your-google-analytics-id
NEXT_PUBLIC_GOOGLE_TAG_MANAGER_ID=your-google-tag-manager-id

# Feature Flags
NEXT_PUBLIC_ENABLE_VOICE_RECOGNITION=true
NEXT_PUBLIC_ENABLE_TEXT_TO_SPEECH=true
NEXT_PUBLIC_ENABLE_CALENDAR_INTEGRATION=true
NEXT_PUBLIC_ENABLE_EMAIL_NOTIFICATIONS=true
NEXT_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
NEXT_PUBLIC_ENABLE_DARK_MODE=true
NEXT_PUBLIC_ENABLE_OFFLINE_MODE=true
NEXT_PUBLIC_ENABLE_PWA=true

# Performance & Caching
NEXT_PUBLIC_CACHE_DURATION=3600
NEXT_PUBLIC_MAX_FILE_SIZE=10485760
NEXT_PUBLIC_ENABLE_COMPRESSION=true
NEXT_PUBLIC_ENABLE_CDN=true

# Security
NEXT_PUBLIC_ENABLE_HTTPS=true
NEXT_PUBLIC_ENABLE_CSP=true
NEXT_PUBLIC_ENABLE_HSTS=true
NEXT_PUBLIC_ENABLE_XSS_PROTECTION=true

# Development Settings
NEXT_PUBLIC_ENABLE_DEBUG_MODE=false
NEXT_PUBLIC_ENABLE_LOGGING=true
NEXT_PUBLIC_LOG_LEVEL=info
NEXT_PUBLIC_ENABLE_HOT_RELOAD=true
NEXT_PUBLIC_ENABLE_SOURCE_MAPS=true

# Testing
NEXT_PUBLIC_ENABLE_TESTING=false
NEXT_PUBLIC_TEST_API_URL=http://localhost:8001
NEXT_PUBLIC_ENABLE_MOCK_DATA=false

# Internationalization
NEXT_PUBLIC_DEFAULT_LOCALE=en
NEXT_PUBLIC_SUPPORTED_LOCALES=en,es,fr,de
NEXT_PUBLIC_ENABLE_I18N=true

# Voice Assistant Settings
NEXT_PUBLIC_DEFAULT_VOICE_ID=alloy
NEXT_PUBLIC_DEFAULT_SPEECH_RATE=normal
NEXT_PUBLIC_DEFAULT_AI_MODEL=gpt-4
NEXT_PUBLIC_DEFAULT_AI_TEMPERATURE=0.7
NEXT_PUBLIC_DEFAULT_AI_MAX_TOKENS=4000

# Calendar Settings
NEXT_PUBLIC_DEFAULT_TIMEZONE=UTC
NEXT_PUBLIC_DEFAULT_WORKING_HOURS=9-17
NEXT_PUBLIC_DEFAULT_WORKING_DAYS=1-5
NEXT_PUBLIC_DEFAULT_MEETING_DURATION=30

# UI/UX Settings
NEXT_PUBLIC_DEFAULT_THEME=light
NEXT_PUBLIC_ENABLE_ANIMATIONS=true
NEXT_PUBLIC_ENABLE_SOUND_EFFECTS=true
NEXT_PUBLIC_ENABLE_HAPTIC_FEEDBACK=true
NEXT_PUBLIC_ENABLE_ACCESSIBILITY=true

# Third-party Services
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
NEXT_PUBLIC_STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

NEXT_PUBLIC_TWILIO_ACCOUNT_SID=your-twilio-account-sid
NEXT_PUBLIC_TWILIO_AUTH_TOKEN=your-twilio-auth-token
NEXT_PUBLIC_TWILIO_PHONE_NUMBER=your-twilio-phone-number

# Database (if using client-side database)
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

NEXT_PUBLIC_FIREBASE_API_KEY=your-firebase-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-firebase-auth-domain
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-firebase-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-firebase-storage-bucket
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your-firebase-messaging-sender-id
NEXT_PUBLIC_FIREBASE_APP_ID=your-firebase-app-id

# WebRTC & Media
NEXT_PUBLIC_ENABLE_WEBRTC=true
NEXT_PUBLIC_ENABLE_MEDIA_RECORDING=true
NEXT_PUBLIC_ENABLE_SCREEN_SHARING=true
NEXT_PUBLIC_ENABLE_CAMERA_ACCESS=true
NEXT_PUBLIC_ENABLE_MICROPHONE_ACCESS=true

# Push Notifications
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your-vapid-public-key
NEXT_PUBLIC_PUSH_SERVER_URL=your-push-server-url

# Error Tracking
NEXT_PUBLIC_ERROR_REPORTING_ENABLED=true
NEXT_PUBLIC_ERROR_REPORTING_URL=your-error-reporting-url

# Development Tools
NEXT_PUBLIC_ENABLE_REDUX_DEVTOOLS=true
NEXT_PUBLIC_ENABLE_REACT_DEVTOOLS=true
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true

# Custom Settings
NEXT_PUBLIC_CUSTOM_FEATURE_FLAG_1=false
NEXT_PUBLIC_CUSTOM_FEATURE_FLAG_2=false
NEXT_PUBLIC_CUSTOM_API_ENDPOINT=your-custom-api-endpoint

# Note: All NEXT_PUBLIC_ variables are exposed to the browser
# Keep sensitive information in server-side environment variables only
